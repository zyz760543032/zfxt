odeDialogVisible\"\n      :show-close=\"false\"\n      :destroy-on-close=\"true\"\n      custom-class=\"codeDialog\"\n      width=\"20%\"\n      center>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <img src=\"../assets/img/code.png\" alt=\"\" style=\"width:100%\"><br>\n        使用微信扫码支付\n      </span>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\n\nexport default {\n\n  data () {\n    return {\n      isActive: 'year',\n      payType: 'wxpay',\n      codeDialogVisible: false\n    }\n  },\n  \n  methods: {\n    selectItem(type) {\n      this.isActive = type\n      //this.$router.push({ path: '/order' })\n    },\n\n    selectPayType(type) {\n      console.log(type)\n      this.payType = type\n      //this.$router.push({ path: '/order' })\n    },\n\n    toPay(){\n        if(this.payType === 'wxpay'){\n            //调用统一下单接口\n            this.codeDialogVisible = true\n        }\n    }\n  }\n}\n</script>\n\n<style>\n.codeDialog .el-dialog__header {\n    padding: 0;\n}\n.codeDialog .el-dialog__body {\n    padding: 5px;\n}\n</style>"]}]}